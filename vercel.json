{
  "version": 2,
  "builds": [
    {
      "src": "app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://parasimback.onrender.com",
    "REACT_APP_UPLOAD_URL": "https://parasimback.onrender.com",
    "REACT_APP_SOCKET_URL": "https://parasimback.onrender.com",
    "REACT_APP_PUBLIC_URL": "https://parasimback.onrender.com/",
    "REACT_APP_USE_STUBS": "false"
  }
}
